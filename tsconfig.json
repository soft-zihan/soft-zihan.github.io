{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "**/*.ts", "**/*.tsx", "**/*.vue"],
  "exclude": ["node_modules", "dist"],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    },
    "verbatimModuleSyntax": true,
    "ignoreDeprecations": "5.0"
  }
}